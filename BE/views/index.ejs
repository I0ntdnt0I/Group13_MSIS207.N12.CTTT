  <link rel="stylesheet" href="main.css">
  <script src="https://kit.fontawesome.com/bea50b74b9.js" crossorigin="anonymous"></script>
</head>
<body>
  <header>
    <div class="nav">
      <div class="nav-left">
        <a href="/" class="nav-logo">Doponline</a>
      </div>
      <div class="nav-fill">
        <input type="text" class="nav-input">
        <div class="nav-search_button">
          <input type="submit" value="Search">
        </div>
      </div>
      <div class="nav-right">
          <div class="nav-function">
            <i class="fa-regular fa-bell"></i>
          </div>
          <div class="nav-function">
            <i class="fa-regular fa-cart-shopping"></i>
          </div>
        <div class="nav-login_form">
          <div class="nav-login_login">
            <a href="/login">Login</a>
          </div>
          <div class="nav-login_register">
            <a href="/register">Register</a>
          </div>
        </div>
      </div>
    </div>
    <div class="menu"></div>
  </header>
  <main></main>
  <footer></footer>
  <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
